
lib libeay32 ssleay32 libeay32-dll ssleay32-dll ;

lib libeay32 : : <file>install/lib/libeay32.lib <toolset>msvc ;
lib ssleay32 : : <file>install/lib/ssleay32.lib <toolset>msvc ;

lib libeay32-dll : libeay32 : <file>install/bin/libeay32.dll <toolset>msvc ;
lib ssleay32-dll : ssleay32 : <file>install/bin/ssleay32.dll <toolset>msvc ;

install ../dist/bin : libeay32-dll ssleay32-dll ;
